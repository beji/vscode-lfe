{
    "name": "lfe",
    "displayName": "LFE",
    "description": "Language support for lfe",
    "version": "1.0.0",
    "publisher": "elevader",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "lfe",
            "aliases": ["LFE", "lfe"],
            "extensions": [".lfe"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "lfe",
            "scopeName": "source.lisp",
            "path": "./syntaxes/lisp.tmLanguage"
        }]
    }
}